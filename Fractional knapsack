#define fos ios_base::sync_with_stdio(false); cin.tie(NULL);
bitset<N> isPrime;
vi Primes;

void PrimeGen(int n)
{
    for(int i=3;i<=n;i+=2)
    {
        isPrime[i]=1;
    }
    int sq = sqrt(n);
    for(int i=3;i<=sq;i+=2)
    {
        if(isPrime[i])
        {
            for(int j = i*i;j<=n;j+=i)
            {
                isPrime[j]=0;
            }
        }
    }
    isPrime[2]=1;
    Primes.pb(2);
    for(int i=3; i<=n;i+=2)
        if(isPrime[i]==1)
        {
            Primes.pb(i);
        }
}

bool isPowerOfTwo(int n) {
    if (n <= 0) return false;
    return (n & (n - 1)) == 0;
}
struct item
{
    double weight,price,unit_price;
};
int main()
{
    fos
    int n,bosta;
    cin>>n>>bosta;
    item a[n];
    loop(i,0,n)
    {
        cin>>a[i].weight>>a[i].price;
        a[i].unit_price=a[i].price/a[i].weight;
    }
    sort(a, a + n, [](const item &x, const item &y){
    return x.unit_price> y.unit_price;
    });
    int i=0,profit=0;
    while(bosta!=0)
    {
        if(bosta>=a[i].weight)
        {
            profit+=a[i].price;
            bosta-=a[i].weight;
            i++;
        }
        else if(bosta<a[i].weight)
        {
            profit+=bosta*a[i].unit_price;
            bosta=0;
        }
    }
    cout<<profit<<'\n';
    return 0;
}
