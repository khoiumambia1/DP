#define fos ios_base::sync_with_stdio(false); cin.tie(NULL);
bitset<N> isPrime;
vi Primes;

void PrimeGen(int n)
{
    for(int i=3;i<=n;i+=2)
    {
        isPrime[i]=1;
    }
    int sq = sqrt(n);
    for(int i=3;i<=sq;i+=2)
    {
        if(isPrime[i])
        {
            for(int j = i*i;j<=n;j+=i)
            {
                isPrime[j]=0;
            }
        }
    }
    isPrime[2]=1;
    Primes.pb(2);
    for(int i=3; i<=n;i+=2)
        if(isPrime[i]==1)
        {
            Primes.pb(i);
        }
}

bool isPowerOfTwo(int n) {
    if (n <= 0) return false;
    return (n & (n - 1)) == 0;
}
int main()
{
    fos
    int n,target;
    cin>>n>>target;
    int coins[n];
    loop(i,0,n)
    {
        cin>>coins[i];
    }
    vector<vector<int>>mat(n+123,vector<int>(target+123));
    loop(i,0,n)
    {
        mat[i][0]=1;
    }
    loop(i,1,n+1)
    {
        loop(j,1,target+1)
        {
           if(j<coins[i-1])
            mat[i][j]=mat[i-1][j];
           else
           {
               mat[i][j]=mat[i-1][j]+mat[i][j-coins[i-1]];
           }
        }
    }
    cout<< mat[n][target]<<'\n';
    loop(i,0,n+1)
    {
        loop(j,0,target+1)
        {
            cout<<mat[i][j]<<' ';
        }
        cout<< '\n';
    }
    return 0;
}
