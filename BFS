#define pqll priority_queue<long long>

#define ordset orderedSet<int, std::string>
#define fos ios_base::sync_with_stdio(false);
bitset<N> isPrime;
vi Primes;

void PrimeGen(int n)
{
    for(int i=3;i<=n;i+=2)
    {
        isPrime[i]=1;
    }
    int sq = sqrt(n);
    for(int i=3;i<=sq;i+=2)
    {
        if(isPrime[i])
        {
            for(int j = i*i;j<=n;j+=i)
            {
                isPrime[j]=0;
            }
        }
    }
    isPrime[2]=1;
    Primes.pb(2);
    for(int i=3; i<=n;i+=2)
        if(isPrime[i]==1)
        {
            Primes.pb(i);
        }
}

bool isPowerOfTwo(int n) {
    if (n <= 0) return false;
    return (n & (n - 1)) == 0;
}
int main()
{
    fos
    cout<< "Enter Number of vertex and edges: ";
    int ver,edg;
    cin>>ver>>edg;

    vector<vector<int>>graph(ver+1,vector<int>(ver+1,0));

    cout<<"\nEnter Edges Start And Ends:\n";
    loop(i,0,edg)
    {
        int x,y;
        cin>>x>>y;
        graph[x][y]=graph[y][x]=1;
    }

    loop(i,1,ver+1)
    {
        loop(j,1,ver+1)
            cout<<graph[i][j]<<' ';
        cout<<"\n";
    }

    int start;
    cout<< "Enter starting vertex: ";
    cin>>start;

    vector<bool> vis(ver+1,false);
    vector<int> vissiq;
    queue<int> b;
    b.push(start);
    vis[start] = true;

    while(!b.empty())
    {
        int u = b.front();
        b.pop();

        vissiq.pb(u);

        loop(i,1,ver+1)
        {
            if (!vis[i] && graph[u][i] == 1)
            {
                vis[i] = true;
                b.push(i);
            }
        }
    }
    cout<< "Visit Sequence: \n";
    for(int x : vissiq)
        cout << x << ' ';
    cout<< "\n";
    return 0;
}
