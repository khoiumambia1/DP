#define ld long double
#define lin(a) \
    lli  a;      \
    cin >> a;
#define lin2(a, b) \
    lli a, b;       \
    cin >> a >> b
#define lin3(a, b, c) \
    lli a, b, c;       \
    cin >> a >> b >> c
#define lin4(a, b, c, d) \
    lli  a, b, c, d;       \
    cin >> a >> b >> c >> d

#define max3(a, b, c) max(max((a), (b)), (c))
#define max4(a, b, c, d) max(max((a), (b)), max((c), (d)))
#define min3(a, b, c) min(min((a), (b)), (c))
#define min4(a, b, c, d) min(min((a), (b)), min((c), (d)))

