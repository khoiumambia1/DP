#define fos ios_base::sync_with_stdio(false); cin.tie(NULL);
bitset<N> isPrime;
vi Primes;

void PrimeGen(int n)
{
    for(int i=3;i<=n;i+=2)
    {
        isPrime[i]=1;
    }
    int sq = sqrt(n);
    for(int i=3;i<=sq;i+=2)
    {
        if(isPrime[i])
        {
            for(int j = i*i;j<=n;j+=i)
            {
                isPrime[j]=0;
            }
        }
    }
    isPrime[2]=1;
    Primes.pb(2);
    for(int i=3; i<=n;i+=2)
        if(isPrime[i]==1)
        {
            Primes.pb(i);
        }
}

bool isPowerOfTwo(int n) {
    if (n <= 0) return false;
    return (n & (n - 1)) == 0;
}
int main()
{
    fos
    string s1,s2;
    getline(cin>>ws,s1);
    getline(cin>>ws,s2);
    int siz1=s1.size();
    int siz2=s2.size();
    vector<vector<int>>mat (siz1+123,vector<int>(siz2+123,0));
    vector<vector<char>>vec (siz1+123,vector<char>(siz2+123,1));
    loop(i,1,siz1+1)
    {
        loop(j,1,siz2+1)
        {
            if(s1[i-1]==s2[j-1])
            {
                mat[i][j]=mat[i-1][j-1]+1;
                vec[i][j]='D';
            }
            else if(mat[i-1][j]>=mat[i][j-1])
            {
                mat[i][j]=mat[i-1][j];
                vec[i][j]='U';
            }
            else if(mat[i-1][j]<mat[i][j-1])
            {
                mat[i][j]=mat[i][j-1];
                vec[i][j]='L';
            }
        }
    }
    cout<< mat [siz1][siz2]<<'\n';
    int i=siz1,j=siz2;
    string ans = "";
    while(i!=0&&j!=0)
    {
        if(vec[i][j]=='D')
        {
            ans+=s1[i-1];
            i--;
            j--;
        }
        else if(vec[i][j]=='L')
        {
            j--;
        }
        else if(vec[i][j]=='U')
        {
            i--;
        }
    }
    reverse(ans.begin(),ans.end());
    cout<<ans;
    return 0;
}
